;;;;;;;;;;


(.t.def ..t.ex

  (.t.is (.t.gen [l (.gen.long)]
           (.t.ex :CAST
                  (conj l
                        42)))
         "Property throws exception with expected code"))



(.t.def ..t.ex..wrong-code
  (.t.is (.t.gen [l (.gen.long)]
           (.t.ex :WRONG-CODE
                  (conj l
                        42)))
         "Property throws exception with unexpected code"))



(.t.def fail

  (.t.is (.t.gen [l (.gen.long)]
           (zero? l))
         "Impossible property"))



(.t.def pass

  (.t.is (.t.gen [l (.gen.long)]
           (long? l))
         "Always true"))



(.t.def thrown

  (.t.is (.t.gen [l (.gen.any)]
           (conj l
                 42))
         "Property throws unexpectedly"))
