;;;;;;;;;;


(.t.def impossible

  (.t.is (.t.gen [d (.gen.double)
                  l (.gen.long)]
           (= d
              l))
         "Impossible property"))



(.t.def thrown

  (.t.is (.t.gen [l (.gen.any)
                  x (.gen.any)]
           (conj l
                 x))
         "Property throws unexpectedly"))



(.t.def thrown.wrong-ex-code
  (.t.is (.t.gen [l (.gen.long)]
           (.t.ex :WRONG-CODE
                  (conj l
                        42)))
         "Property throws exception with unexpected code"))
