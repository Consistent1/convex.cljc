  }




 :paths
 [;"project/break/src/clj/main"
  ;"project/break/src/cvx/main"
  ;"project/clojurify/src/cljc/main"
  ;"project/cvm/src/clj/main"
  ;"project/run/src/clj/main"
  ;"project/run/src/cvx/main"
  ;"project/sync/src/clj/main"
  ;"project/watch/src/clj/main"
  ]
}
